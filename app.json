{
  "expo": {
    "name": "rivalry-hub",
    "slug": "rivalry-hub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wthew.rivalryhub"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-community-flipper"
    ],
    "experiments": {
      "typedRoutes": true,
      "baseUrl": "/rivalry-hub"
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "f7a8aa17-3038-47e4-976a-729b52b881ee"
      }
    }
  }
}
